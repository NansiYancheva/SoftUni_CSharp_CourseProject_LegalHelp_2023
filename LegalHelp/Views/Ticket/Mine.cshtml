@model List<TicketAllViewModel>
@{
    ViewData["Title"] = "My Tickets";
}

<h2 class="text-center">@ViewData["Title"]</h2>
<hr/>

@if (!Model.Any())
{
    <h2 class="text-center">You do not have any tickets yet!</h2>
}
else
{
    <div class="row">
        @foreach (TicketAllViewModel ticket in Model)
        {
            <partial name="_TicketPartial" model="ticket" />
        }
    </div>
}
